{{>layouts/header}}
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">title</th>
            <th scope="col">content</th>
        </tr>
    </thead>
    <tbody>
        {{#articleList}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/articles/{{id}}">{{#title}}{{title}}{{/title}}
                {{^title}}널입니다.{{/title}}</a></td>
            <td>{{#content}}{{content}}{{/content}}
                {{^content}}널입니다.{{/content}}</td>
        </tr>
        {{/articleList}}
    </tbody>
</table>
<a href="/articles/new">New Article</a>
{{>layouts/footer}}